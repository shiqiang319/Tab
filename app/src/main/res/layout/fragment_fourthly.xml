            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">
            <CheckBox
                android:id="@+id/cb_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="清污"
                android:textSize="20sp"
                android:layout_marginLeft="50dp"/>
            <CheckBox
                android:id="@+id/cb_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
