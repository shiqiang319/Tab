                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginLeft="290dp"
                android:hint="P4"
                android:maxLength="3"
                android:maxLines="1"
                android:inputType="number"
                android:background="@drawable/edit_bg"/>
