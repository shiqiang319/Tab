            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="设 备 配 置"
                android:textColor="#000000"
                android:textSize="20sp"
                android:layout_marginLeft="140dp"/>
        </RelativeLayout>
        <RelativeLayout
